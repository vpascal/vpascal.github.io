library(readxl); library(dplyr); library(tidyr);
data_2014 <- read_excel("Excel/Faculty Credits AY 2014-15_29JAN16.xlsx",sheet = "Faculty_Load_2014_2015")[c(2,7,30,32)]
data_2014$Year <- 2014
data_2013 <- read_excel("Excel/Faculty Load Report AY 2013-14_20APR16.xlsx",sheet = "Faculty_Load_2013_2014")[c(2,7,29,31)]
data_2013$Year <- 2013
names(data_2013) <- names(data_2014)
data <- bind_rows(data_2013,data_2014)
data_summary <- data %>% dplyr:: filter(`Dept Code`!="ST") %>%
group_by(`Dept Code`,`Faculty Type`, Year) %>%
summarise(Total=sum(`Difference AY`)) %>% mutate(Percent=round(Total/sum(Total)*100,1)) %>%
rename(Dept = `Dept Code`,Faculty=`Faculty Type`)
data_summary_ii <- data %>% dplyr:: filter(`Dept Code`!="ST") %>%
group_by(`Dept Code`, Year) %>%
summarise(Uncompleted=sum(`Difference AY`), Total=sum(`Total Credits AY`)) %>%
mutate(`Percent Uncompleted` = round(Uncompleted/Total*100,1))
